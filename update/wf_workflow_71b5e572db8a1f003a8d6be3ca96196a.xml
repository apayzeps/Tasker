        <value>answer = generateSubTaskerDefs();&#13;
function generateSubTaskerDefs() {&#13;
	workflow.scratchpad.u_Task_route_order_count +=1;&#13;
	&#13;
gs.debug('DiscoverTasker - Save Template - Saving Sub Taskers - processing Route Order:' + workflow.scratchpad.u_Task_route_order_count);&#13;
	&#13;
gs.debug('DiscoverTasker - Save Template - Saving Sub Taskers for parent: ' + current.original_tasker_inst);&#13;
	&#13;
	var taskInstanceQuery = new GlideRecord('x_153465_discovert_route_task_instance');&#13;
	taskInstanceQuery.addQuery('parent',current.original_tasker_inst).addCondition('order',workflow.scratchpad.u_Task_route_order_count);&#13;
	taskInstanceQuery.query();&#13;
	&#13;
	var wf = new global.Workflow();&#13;
	while (taskInstanceQuery.next()){&#13;
gs.debug('DiscoverTasker - Save Template - Saving Sub Taskers - Tasks Loaded:' + workflow.scratchpad.u_Tasks_loaded_count);&#13;
		var recTaskDef = new GlideRecord('x_153465_discovert_route_tasks');&#13;
		recTaskDef.initialize();&#13;
		recTaskDef.parent = current.sys_id;&#13;
		recTaskDef.tasker_process_definition = current.tasker_process_definition;&#13;
		 &#13;
		recTaskDef.original_tasker_inst = taskInstanceQuery.sys_id;&#13;
		recTaskDef.insert();&#13;
&#13;
		var  wfId = wf.getWorkflowFromName("Discover Save Tasks to Process");&#13;
 		wf.startFlow(wfId, recTaskDef, 'Update');&#13;
&#13;
		// increment the total routes processed for each route in this order&#13;
		workflow.scratchpad.u_Tasks_loaded_count +=1;&#13;
	}&#13;
}</value>
