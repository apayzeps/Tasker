<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Removes "Global Process, OU Process" options from process template forms for users who do not have the required roles to create/view/run  those types of process templates</description>
        <field>schedules</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Tasker -  Restrict New Process Template </name>
        <order/>
        <script><![CDATA[function onLoad() {
	if(!g_user.hasRole('x_153465_discovert.tasker_process_template_global') || !g_user.hasRole('x_153465_discovert.tasker_process_template_ou') ){
		if(g_form.getValue('template_name') == "")
			g_form.removeOption('process_level','global_process');
		g_form.removeOption('process_level','ou_process');
		
		if(g_form.getValue('template_name') != ""  ){
			g_form.removeOption('process_level','global_process');
			g_form.removeOption('process_level','ou_process');
		}
	}
}]]></script>
