		case  1: return 200;  // Assigned
		case 10: return 300;  // Accepted
		case 11: return 100;  // Rejected - Set back to Pending Assingment for Parent
		case 12: return 400;  // Work Scheduled
		case 13: return 500;  // Work in Progress
		case 14: return 600;  // In Review
		case 15: return 500;  // ReWork  - Set back to Work in Progress
		case  3: return 900;  // Closed Complete
		case  4: return 910;  // Closed Incomplete
		case  7: return 920;  // Cancel
		default: return 100;  // Undefiend State - set to pending assingment
	}
	
})(current);]]></calculation>
