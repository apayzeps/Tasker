        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Add your code here
	var grNewWSHistory = new GlideRecord('x_153465_discovert_war_sar_history');
	grNewWSHistory.initialize();
	grNewWSHistory.activity_id = current.sys_id;
	grNewWSHistory.current_assignee_ou = current.assignee_ou;
	grNewWSHistory.previous_assignee_ou = previous.assignee_ou;
	grNewWSHistory.current_assignee = current.assignee;
	grNewWSHistory.previous_assignee = previous.assignee;
	grNewWSHistory.action = current.status.getDisplayValue();
	// pull the OU so we can grab the abbreviation for the work note.
	var grOU = new GlideRecord('x_153465_discovert_discover_office_hierarchy');
	grOU.get(current.assignee_ou);
	
	grNewWSHistory.notes = 'Activity ID: ' + current.number + 'Action: ' + current.status + ' Assignee: ' + current.assignee.name + ' Assignee OU: ' + grOU.office_abbreviation;

	grNewWSHistory.insert();
	
	
	// Add your code here

})(current, previous);]]></script>
