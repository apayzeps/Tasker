        <onclick/>
        <order>100</order>
        <script><![CDATA[// NOTE RAJ - this will call an event to launch a process to recurse the tree and save the template.
if(typeof window == 'undefined'){
	saveTemplate();
	gs.addInfoMessage('DiscoverTasker saving Tasker as a Process Template');
}

function saveTemplate()
{
	// start the process of creating a new process definition
	gs.eventQueue('x_153465_discovert.DT_Save_as_Process', current, 'Save as Process',  gs.getUserID());
	
}

]]></script>
