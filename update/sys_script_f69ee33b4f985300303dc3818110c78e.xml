        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	// RAJ 2018-02-22  STRY0010778 - updated to replace route definition and recursively push demo data change to all task definitions
	// Query the database and find all subTask definitions
	var grSubTaskDefs = new GlideRecord('x_153465_discovert_route_tasks');
	grSubTaskDefs.addQuery('parent',current.sys_id);
	grSubTaskDefs.query();
	while(grSubTaskDefs.next()){
		grSubTaskDefs.demo_data = current.demo_data;
		grSubTaskDefs.update();
	}
	
})(current, previous);]]></script>
